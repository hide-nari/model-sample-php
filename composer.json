{
  "name": "hidenari/model-sample",
  "description": "model sample",
  "license": "MIT",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Hidenari\\ModelSample\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
